<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.seth.addhelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <uses-permission android:name="android.presentation.MODIFY_AUDIO_SETTINGS" />

        <activity
            android:name=".ViewTasksActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddTasksActivity"></activity>
        <activity android:name=".OverviewActivity"></activity>
    </application>
<!--android.permission.MODIFY_AUDIO_SETTINGS-->
</manifest>
